 class=\"flex-shrink-zero mr2 lazy-image ember-view\"\n        />\n        <div class=\"full-width\">\n          <h4 class=\"t-16 t-black t-bold\">{{ item.position }}</h4>\n          <p class=\"t-16 t-black\">\n            {{ item.name }}\n            <span\n              v-show=\"item.type !== undefined && item.type.trim().length > 0\"\n              class=\"pv-entity__secondary-title separator\"\n            >{{ item.type }}</span>\n          </p>\n          <p class=\"t-14 t-black\">\n            {{ getDuration(item.start, item.end) }}\n            <span\n              v-show=\"item.location !== undefined && item.location.trim().length > 0\"\n              class=\"pv-entity__secondary-title separator\"\n            >{{ item.location }}</span>\n          </p>\n\n          <p class=\"white-space-pre-wrap break-words t-14 t-black\">\n            <span dir=\"ltr\" v-html=\"item.description\" />\n          </p>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport { Experience } from \"../data/data\";\nexport default {\n  name: \"CvExperience\",\n  data() {\n    return {\n      data: {}\n    };\n  },\n  methods: {\n    getLogoAlt(name) {\n      return `Logo for ${name}`;\n    },\n    getDuration(start, end) {\n      return end === \"\" ? `${start} – Present` : `${start} – ${end}`;\n    }\n  },\n  computed: {},\n  mounted() {\n    this.$nextTick(() => {\n      this.data = Experience;\n    });\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.pv-entity__secondary-title.separator::before {\n  content: \"\\00B7\";\n  padding: 0 4px 0 0;\n}\n</style>\n"]}]}